{
	"routes": {

		"/home/": {
			"template": "index"
		},

		"/feed/content/recent.rss": {
			"template": "contents.rss",
			"filters": [ "order_by=published_on", "order_direction=desc" ]
		},

		"/feed/albums/:id/recent.rss": {
			"template": "album.rss",
			"filters": [ "order_by=published_on", "order_direction=desc" ]
		},

		"/feed/essays/recent.rss": {
			"template": "essays.rss",
			"filters": [ "order_by=published_on", "order_direction=desc" ]
		},

		"/feed/timeline/recent.rss": {
			"template": "timeline.rss"
		}

	},

